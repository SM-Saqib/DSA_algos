class Solution(object):
    def runningSum(self, nums):
        """
        :type nums: List[int]
        :rtype: List[int]
        """
        if len(nums) > 0 and len(nums) <1001 and min(nums) > -1000000 and max(nums) < 1000000:
            run_sum = [int(sum(nums[:i])) for i in range(1,len(nums)+1)]
            print(run_sum)
        return run_sum
        
